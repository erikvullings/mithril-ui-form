{
  "name": "mithril-ui-form-example",
  "private": true,
  "version": "0.0.1",
  "description": "Example project, showing how to use the Mithril UI form component.",
  "main": "index.js",
  "target": "web",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm start",
    "start": "webpack serve --env development --open",
    "build": "webpack --env production",
    "build:domain": "npm run clean && npm run build",
    "build:old": "npm run clean && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-ui-form"
  },
  "keywords": [
    "mithril",
    "tree",
    "component",
    "typescript"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/mithril-ui-form/issues"
  },
  "homepage": "https://github.com/erikvullings/mithril-ui-form#readme",
  "dependencies": {
    "material-icons": "^1.11.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.2.2",
    "mithril-materialized": "^0.19.6",
    "mithril-ui-form": "workspace:^1.1.8",
    "mithril-ui-form-plugin": "workspace:^0.3.3",
    "mithril-ui-form-leaflet-plugin": "workspace:^0.3.0",
    "mithril-ui-form-rating-plugin": "workspace:^0.4.0"
  },
  "devDependencies": {
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "webpack": "^5.72.1",
    "@types/materialize-css": "^1.0.11",
    "@types/mithril": "^2.0.11",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  }
}
