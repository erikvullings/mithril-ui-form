{
  "name": "mithril-ui-form-example",
  "private": true,
  "version": "0.0.1",
  "description": "Example project, showing how to use the Mithril UI form component.",
  "main": "index.js",
  "target": "web",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm start",
    "start": "webpack serve --env development --open",
    "build": "webpack --env production",
    "build:domain": "npm run clean && npm run build",
    "build:old": "npm run clean && parcel build index.html --out-dir ../../docs --public-url https://erikvullings.github.io/mithril-ui-form"
  },
  "keywords": [
    "mithril",
    "tree",
    "component",
    "typescript"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/mithril-ui-form/issues"
  },
  "homepage": "https://github.com/erikvullings/mithril-ui-form#readme",
  "dependencies": {
    "material-icons": "^1.13.14",
    "mithril": "^2.3.4",
    "mithril-materialized": "beta",
    "mithril-ui-form": "workspace:*",
    "mithril-ui-form-leaflet-plugin": "workspace:*",
    "mithril-ui-form-rating-plugin": "workspace:*"
  },
  "devDependencies": {
    "@types/mithril": "^2.2.7",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.1",
    "html-webpack-plugin": "^5.6.4",
    "html-webpack-tags-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.9.4",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "5.9.2",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}